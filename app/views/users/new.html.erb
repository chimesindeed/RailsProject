<%= render 'shared/head' %>
<%= render 'shared/flash_messages' %>
<h1>Sign Up</h1>
<%= form_for @user, controller: 'users', action: 'create' do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "create" %>

<% end %>  
